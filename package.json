{
  "name": "@larmbox/elements",
  "version": "0.0.28",
  "description": "Larmbox Elements is a Vue 3 component library.",
  "license": "MIT",
  "main": "dist/elements.umd.js",
  "module": "dist/elements.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/elements.umd.js",
      "import": "./dist/elements.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/": {
      "require": "./dist/",
      "import": "./dist/"
    },
    "./nuxt/": {
      "require": "./nuxt/dist/module.cjs",
      "import": "./nuxt/dist/module.mjs",
      "types": "./nuxt/dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "nuxt/dist"
  ],
  "web-types": "dist/web-types.json",
  "vetur": {
    "tags": "dist/vetur-tags.json",
    "attributes": "dist/vetur-attributes.json"
  },
  "sideEffects": false,
  "scripts": {
    "build-nuxt-module": "nuxt-module-build ./nuxt",
    "build": "npm run lint:fix && npm run test && npm run build:dangerous",
    "build:dangerous": "cross-env NODE_ENV=production ts-node build/pre-build && vite build --config build/vite.config.js && tsc --p tsconfig.build.json --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.build.json && npm run build:sass && ts-node -r tsconfig-paths/register build/post-build",
    "build:sass": "sass ./dist:./dist",
    "build:vuepress": "vuepress build docs",
    "deploy": "build/deploy-docs.sh",
    "publish:npm": "npm i && npm run build && npm publish --access public",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue --fix .",
    "test": "vitest run --config ./build/vite.config.js",
    "test:watch": "vitest --config ./build/vite.config.js",
    "coverage": "vitest run --coverage --config ./build/vite.config.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@nuxt/kit": "^3.0.0",
    "@nuxt/module-builder": "^0.1.7",
    "@types/fs-extra": "^9.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^8.0.1",
    "fs-extra": "^10.0.0",
    "happy-dom": "^2.34.0",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.6.6",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.7",
    "vue": "^3.2.45"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0"
  }
}
